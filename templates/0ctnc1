<script type="d46ba5d153b1764410536b7c-text/javascript">
		$(document).ready(function(){
			var method = "regular";

			$('#password-modal').modal({
				'dismissable': false
			});

			$(".start-download").click(function(e) {
				method = "regular";
									createDownloadToken("");
									return false;
			});

			$(".start-turbo-download").click(function(e) {
				method = "turbo";
									$('#password-modal .password-column').addClass("hide");
					$('#password-modal p').addClass("hide");
					createDownloadToken("");
									return false;
			});

			$("#password-modal button.btn").click(function() {
				var password = $("#password").val();
				createDownloadToken(password);
			});

			function createDownloadToken(value) {
				loader($("#password-modal .modal-content"));
				$.ajax({
					type: "POST",
					url: "https://eu5.easyupload.io/action.php",
					data: {
						type: "download-token",
						url: "0ctnc1",
						value: value,
						method: method
					},
					xhrFields: {
						withCredentials: true
					},
					crossDomain: true,
					dataType: "json",
					success: function(r) {
						if ( r.status ) {
							$("#password-modal .error").html('<img class="icon" src="https://easyupload.io/img/check.png" />' + r.data);
							// $(".download-iframe").attr('src', r.download_link);
							window.location = r.download_link;
							$('#password-modal').modal('close');
						} else {
							$("#password-modal .error").html('<img class="icon" src="https://easyupload.io/img/warning.png" />' + r.data);
							if ( r.data == "1" ) {
								$('#password-modal').modal('close');
								$('#download-error').modal('open');
							}
						}
						loaderclose($("#password-modal .modal-content"));
					}
				});
			}
		});
		</script>
<script type="d46ba5d153b1764410536b7c-text/javascript">
		var i = 0;
		function isActive(order_id) {
			$.ajax({
				url: 'https://easyupload.io/action.php',
				type: 'POST',
				data: { type: "isActive", order_id: order_id },
				dataType: 'json',
				cache: false,
				success: function(r) {
					if ( r.status ) {
						location.reload();
						// loaderclose('html');
					} else {
						setTimeout(function() {
							isActive(order_id);
						}, 1000);
						i++;
						if ( i > 30 ) {
							location.reload();
							// loaderclose('html');
						}
					}
				}
			});
		}

		function onPopupClose(order) {
			if ( order != null ) {
				loader('html');
				isActive(order.id);
			}
		}
		</script>
